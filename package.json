{
  "name": "nemp3",
  "version": "2.1.0",
  "description": "A music download store using payment and token assets on the NEM blockchain.",
  "repository": "https://github.com/csleary/nemp3",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon --ignore client/ -r dotenv/config index.js dotenv_config_path=dev.env",
    "production": "nodemon --ignore client/ -r dotenv/config index.js dotenv_config_path=prod.env",
    "client": "yarn --cwd ./client/ start",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "prod": "concurrently \"yarn run production\" \"yarn run client\""
  },
  "author": "Christopher Leary",
  "license": "ISC",
  "dependencies": {
    "@stomp/rx-stomp": "^1.0.2",
    "amqplib": "^0.8.0",
    "archiver": "^5.3.0",
    "aws-sdk": "^2.922.0",
    "axios": "^0.21.2",
    "bcrypt": "^5.0.0",
    "connect-busboy": "0.0.2",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.3.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "human-id": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.13",
    "nem-sdk": "^1.6.7",
    "nodemailer": "^6.4.18",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-spotify": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "rxjs": "^7.1.0",
    "sax": "^1.2.4",
    "sharp": "^0.30.5",
    "slugify": "^1.4.5",
    "socket.io": "^4.0.1",
    "worker-threads-pool": "^2.0.0",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.7"
  }
}
